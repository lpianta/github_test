    "def counter():\n",
    "    number = int(input(\"Enter a number: \"))\n",
    "    counter = str(\"\")\n",
    "    for i in range(0, number + 1):\n",
    "        counter += str(i) + \" \"\n",
    "    print(f\"The count from 0 to {number} is: {counter}\")\n",
